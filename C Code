#include <stdio.h>
#include <stdlib.h>

int d;
Divider(int x){

    do{
        if (d%x==0){
            d=d/x;
            printf("%d\t",x);
        }
        else
            Divider(x+1);




    }
    while(x<d);

}

int main()
{
    printf("Enter a number: ");

    scanf("%d",&d);
    Divider(2);
    return 0;
}
